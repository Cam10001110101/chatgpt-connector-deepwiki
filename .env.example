# Copy this file to .env and fill in your actual values

# OpenAI API Key (required for all tests)
OPENAI_API_KEY=your_openai_api_key_here

# Public DeepWiki MCP Server (no authentication required)
SERVER_URL_SSE=https://your-worker.domain/sse
SERVER_URL_MCP=https://your-worker.domain

# Your GitHub MCP Server Configuration (requires authentication)
GITHUB_MCP_SERVER_URL=https://your-worker-name.your-subdomain.workers.dev
GITHUB_ACCESS_TOKEN=your_github_personal_access_token_here
